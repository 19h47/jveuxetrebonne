{% apply spaceless %}
{% verbatim %}
<li id="Instagram-post-{{model.id}}" class="Instagram-post col-12 col-md-2">
    <a class="" href="{{model.link}}" target="_blank">
        <img class="thumbnail" src="{{model.images.standard_resolution.url}}" alt="{{model.caption.text}}" width="{{model.images.standard_resolution.width}}" height="{{model.images.standard_resolution.height}}"/>
        <span class="count">{{model.likes.count}}</span>
    </a>
</li>
{% endverbatim %}
{% endapply %}
